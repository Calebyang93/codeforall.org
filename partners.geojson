---
---
{
"features": [
{% include partners.liquid %}
 {% for org in partners %}
 {
   "geometry": {
     "coordinates": [
       {{ org.longitude }},
       {{ org.latitude }}
     ],
     "type": "Point"
   },
   "id": "{{ org.name | slugify }}",
   "properties": { {% for p in org %}
     "{{ p[0] }}": {{ p[1] | jsonify }},{% endfor %}

     {% capture logo_url %}/assets/images/partner logos/{{ org.name | slugify }}.jpg{% endcapture %}
     "logo_url": "{{ logo_url }}"
   },
   "type": "Feature"
 }
 {% unless forloop.last %},{% endunless %}
 {% endfor %}
]}
